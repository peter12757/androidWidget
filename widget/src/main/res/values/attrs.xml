<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NiftySlider">
        <attr name="android:value" />
        <attr name="android:valueFrom" />
        <attr name="android:valueTo" />
        <attr name="android:stepSize" />
        <attr name="android:layout_height" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="trackColor" format="color" />
        <attr name="trackSecondaryColor" format="color"/>
        <attr name="trackColorInactive" format="color" />
        <attr name="ticksColor" format="color" />
        <attr name="ticksColorInactive" format="color" />
        <attr name="ticksVisible" format="boolean" />
        <attr name="tickRadius" format="dimension" />
        <attr name="trackHeight" format="dimension"/>
        <attr name="trackInnerHPadding" format="dimension"/>
        <attr name="enableAutoHPadding" format="boolean"/>
        <attr name="trackInnerVPadding" format="dimension"/>
        <attr name="trackCornersRadius" format="dimension"/>
        <attr name="thumbColor" format="color"/>
        <attr name="thumbRadius" format="dimension"/>
        <attr name="thumbWidth" format="dimension"/>
        <attr name="thumbHeight" format="dimension"/>
        <attr name="thumbVOffset" format="dimension"/>
        <attr name="thumbElevation" format="dimension"/>
        <attr name="thumbStrokeColor" format="color" />
        <attr name="thumbStrokeWidth" format="dimension" />
        <attr name="thumbShadowColor" format="color" />
        <attr name="thumbWithinTrackBounds" format="boolean"/>
        <attr name="thumbText" format="string"/>
        <attr name="thumbTextColor" format="color"/>
        <attr name="thumbTextSize" format="dimension"/>
        <attr name="thumbTextBold" format="boolean"/>
        <attr name="enableDrawHalo" format="boolean"/>
        <attr name="haloColor" format="color"/>
        <attr name="haloRadius" format="dimension"/>
        <attr name="android:hapticFeedbackEnabled" format="boolean"/>
        <attr name="tipViewVisible" format="boolean"/>
        <attr name="tipViewVerticalOffset" format="dimension"/>
        <attr name="tipViewBackground" format="color"/>
        <attr name="tipViewTextColor" format="color"/>
        <attr name="tipTextAutoChange" format="boolean"/>
        <attr name="isTipViewClippingEnabled" format="boolean"/>
        <attr name="enableProgressAnim" format="boolean"/>
        <attr name="isConsecutiveProgress" format="boolean"/>
        <attr name="sliderTouchMode">
            <enum name="normal" value ="0"/>
            <enum name="disableTouch" value="1"/>
            <enum name="disableClickTouch" value="2"/>
        </attr>
    </declare-styleable>
</resources>